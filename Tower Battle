local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()

local Window = Library.CreateLib("Сало скрипт (ержык)", "RJTheme1")

local Tab = Window:NewTab("Игрок")

local Section = Tab:NewSection("Скорость")

Section:NewButton("Скорость обычная", "Изменяет скорость на обычную", function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    
end)

Section:NewButton("Скорость 25", "Изменяет скорость на 25", function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25
    
end)

